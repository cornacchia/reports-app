<ul class="navbar">
  <li><a href="/admin/userList">Lista utenti</a></li>
  <li><a class="active" href="/admin/reportsList">Lista rapportini</a></li>
  <li><a href="/admin/manage">Amministra database</a></li>
  <li class="logout"><a href="/logout">Logout</a></li>
</ul>

<div class="row">
  <form action="/admin/exportCsv" method="get">
    <button class="pull-right">Esporta csv</button>
  </form>
</div>

<table>
  <thead>
    <tr>
      <th>Utente</th>
      <th>Data</th>
      <th>Sito</th>
      <th>Inizio lavoro</th>
      <th>Pausa pranzo</th>
      <th>Fine lavoro</th>
      <th>Ore di viaggio</th>
      <th>Ore di lavoro</th>
      <th>Veicolo</th>
      <th>Kilometri percorsi</th>
    </tr>
  </thead>
  <tbody>
    {{#each elements as |element|}}
      <tr>
        <td>{{element.user}}</td>
        <td>{{element.date}}</td>
        <td>{{element.site}}</td>
        <td>{{element.workStarted}}</td>
        <td>{{element.workPaused}}</td>
        <td>{{element.workStopped}}</td>
        <td>{{element.travelTime}}</td>
        <td>{{element.workHours}}</td>
        <td>{{element.vehicle}}</td>
        <td>{{element.vehicleKm}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>